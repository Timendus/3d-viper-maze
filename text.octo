: main
  v1 := 0
  v3 := 10
  v4 := 10
  print-text1
  v1 := 0
  v3 := 10
  v4 := 16
  print-text2
  loop
  again

: print-text1
  i := text1
  draw-next-char
  if v0 == 0 then return
  jump print-text1

: print-text2
  i := text2
  draw-next-char
  if v0 == 0 then return
  jump print-text2

: draw-next-char
  i += v1
  load v0
  if v0 == 0 then return
  i := font
  v0 <<= v0
  i += v0
  i += v0
  i += v0
  load v0
  sprite v3 v4 5
  v3 += v0
  v1 += 1
  return

:stringmode ascii "\0 !\"#$%&'()*+,-./0123456789:;<=>?@ABCDEFGHIJKLMNOPQRSTUVWXYZ[\\]^_`abcdefghijklmnopqrstuvwxyz{|}~" {
  :byte { VALUE }
}

: text1
  ascii "Hello world" 0

: text2
  ascii "Whaddayaknow" 0

