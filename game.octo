  0b01010000
  0b10100000

# player starting position
  0b11110000
  0b11010000
  0b11010000
  0b11110000

  0b00000000
  0b01100000
  0b01100000
  0b00000000

# finish
  0b11110000
  0b10010000
  0b11110000
  0b11110000

  0b11110000
  0b10010000
  0b11110000
  0b11110000

: player-tile

# Up
